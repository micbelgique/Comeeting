<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.les4elefantastiq.les4elefantcowork">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">

        <activity android:name=".activities.NavigationActivity" android:screenOrientation="nosensor"  >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".activities.SignInActivity" android:screenOrientation="nosensor" />
        <activity android:name=".activities.CoworkersActivity" android:screenOrientation="nosensor" />
        <activity android:name=".activities.CoworkspaceActivity" android:screenOrientation="nosensor" />
        <activity android:name=".activities.CoworkerActivity" android:screenOrientation="nosensor" />
        <activity android:name=".activities.CoworkspaceDetailsActivity" android:screenOrientation="nosensor" />

    </application>


</manifest>